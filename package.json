{
  "name": "yt-dlp-dl",
  "version": "1.0.0",
  "description": "Downloads the latest version of yt-dlp",
  "repository": "https://github.com/Shingyx/yt-dlp-dl",
  "license": "MIT",
  "author": "Su-Shing Chen",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": "dist/cli.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "yarn clean",
    "build": "tsc",
    "clean": "rimraf dist",
    "prepare": "yarn build",
    "prepublishOnly": "yarn run-checks",
    "lint": "eslint --ext .ts src",
    "fix-lint": "yarn lint --fix",
    "check-format": "prettier --check \"./**/*.{json,md,ts}\" --ignore-path .gitignore",
    "format": "yarn check-format --write",
    "run-checks": "yarn lint && yarn check-format",
    "fix-checks": "yarn fix-lint && yarn format"
  },
  "dependencies": {
    "node-fetch": "^2.6.7",
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@types/node": "^14.17.17",
    "@types/node-fetch": "^2.5.12",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.4.3"
  }
}
